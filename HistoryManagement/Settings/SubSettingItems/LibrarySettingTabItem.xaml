<local:SettingBaseTabItem x:Class="HistoryManagement.Settings.SubSettingItems.LibrarySettingTabItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:HistoryManagement.Settings.SubSettingItems"
             xmlns:fileExplorer="clr-namespace:FileExplorer;assembly=FileExplorer"
             Header="{StaticResource IDS_SETTINGS_HEADER_CHOOSELIBRARY}" Style="{StaticResource SettingTabItemStyle}"
             Loaded="SettingBaseTabItem_Loaded" Unloaded="SettingBaseTabItem_Unloaded">
    <Grid>
        <fileExplorer:UITreeView DataContext="{Binding TreeViewModel}" />
    </Grid>
</local:SettingBaseTabItem>
