<customDialog:CommonDialog x:Class="HistoryManagement.Settings.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HistoryManagement"
        xmlns:customDialog="clr-namespace:Theme.CustomControl.Dialog;assembly=Theme"
        xmlns:tabItems="clr-namespace:HistoryManagement.Settings.SubSettingItems"
        WindowStartupLocation="CenterOwner"  Height="432" Width="702"  CanMinimize="Collapsed" >
    <Grid Background="#f2f2f2">
        <TabControl Style="{DynamicResource SettingTabControlStyle}" SelectedIndex="{Binding SelectedIndex}">
            <tabItems:ImportHistoryFilsSettingTabItem />
            <tabItems:AddLibrarySettingTabItem />
        </TabControl>
    </Grid>
</customDialog:CommonDialog>
